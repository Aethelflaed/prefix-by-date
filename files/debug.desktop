[Desktop Entry]
Name=Desktop actions
Type=Service
MimeType=application/pdf;image/jpeg;
Actions=prefixByDate;prefixByToday;

[Desktop Action prefixByDate]
Name=Prefix by date
Icon=edit-entry
Exec=systemd-run --user /home/geoffroy/src/prefix-by-date/target/debug/prefix-by-date -vvv %F

[Desktop Action prefixByToday]
Name=Prefix by today's date
Icon=edit-entry
Exec=systemd-run --user /home/geoffroy/src/prefix-by-date/target/debug/prefix-by-date --today -vvv %F
